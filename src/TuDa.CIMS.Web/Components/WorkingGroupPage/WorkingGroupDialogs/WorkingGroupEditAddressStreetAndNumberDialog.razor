@using TuDa.CIMS.Shared.Entities
<MudDialog>
    <TitleContent>
        Straße und Hausnummer bearbeiten
    </TitleContent>
    <DialogContent>
        <MudTextField T="String" @bind-Value="ProfessorStreet" Label="Street" Required="true"/>
        <MudTextField T="int" @bind-value="ProfessorNumber" Label="Number" Required="true"/>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel" Color="Color.Error"> Abbrechen</MudButton>
        <MudButton OnClick="Save" Color="Color.Success"> Speichern</MudButton>
    </DialogActions>
</MudDialog>
