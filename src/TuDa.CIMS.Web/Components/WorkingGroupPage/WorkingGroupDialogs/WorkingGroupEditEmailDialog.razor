<MudDialog>
    <TitleContent>
        Email-Adresse bearbeiten
    </TitleContent>
    <DialogContent>
        <MudTextField T="String" @bind-Value="ProfessorEmailAddress" Label="EmailAddress" Required="true"/>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel" Color="Color.Error"> Abbrechen</MudButton>
        <MudButton OnClick="Save" Color="Color.Success"> Speichern</MudButton>
    </DialogActions>
</MudDialog>
